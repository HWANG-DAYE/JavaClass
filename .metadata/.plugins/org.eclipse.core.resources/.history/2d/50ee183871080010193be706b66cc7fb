package EX;

public class ForEx07 {
	public static void main(String[] args) {
		System.out.print("*");
		System.out.print("*");
		System.out.print("*");
		System.out.println();

		System.out.print("*");
		System.out.print("*");
		System.out.print("*");
		System.out.println();

		System.out.print("*");
		System.out.print("*");
		System.out.print("*");
		System.out.println();

		System.out.print("*");
		System.out.print("*");
		System.out.print("*");
		System.out.println();

		System.out.println("-----------------");

		for (int i = 1; i <= 3; i++) {
			System.out.print("*");
		}

		for (int i = 1; i <= 3; i++) {
			System.out.print("*");
		}

		for (int i = 1; i <= 3; i++) {
			System.out.print("*");
		}

		for (int i = 1; i <= 3; i++) {
			System.out.print("*");
		}

		System.out.println("-----------------");

		for (int z = 1; z <= 4; z++) {
			for (int i = 1; i <= 3; i++)
				System.out.print("*");
			}
			System.out.println();

	}
}
