/**
 * sum 합 구하기
 * 평균(합/배열의 길이) -> 소수점까지 출력
 */
package project0327;

public class Array06_Random {
	public static void main(String[] args) {
		int sum = 0;
		int[] arr = new int[5];

		for (int i = 0; i < arr.length; i++) {
			arr[i] = (int) (Math.random() * 6) + 1;
			System.out.println("arr[" + i + "] " + arr[i]);
			System.out.println("평균 : " + 5/sum);
		}
	}
}
