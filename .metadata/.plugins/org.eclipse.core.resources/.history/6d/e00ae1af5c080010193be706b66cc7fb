package EX;

import java.util.Scanner;

public class PrintEx03 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("수를 입력하시오: ");
		System.out.println("수를 입력하시오: ");
		int n = sc.nextInt();
		
		for (int i=1; i<=n; i++) {
			System.out.printf("", args)
		}
	}
}
