package project0328;

public class Array01 {
	public static void main(String[] args) {
//	int[] arr = { 1, 2, 3 };
//	
//	int[] arr2;
//	arr2 = new int[] { 1, 3, 5 };
//	
//	int [] arr3 = new int[5];
//	--------------------------------------------------------------

/**
 * 길이가 5인 배열을 선언 
 * 1~6 사이의 랜덤한 값으로 초기화 
 * 값을 출력 
 * 합, 평균, 최대, 최소 구한 후 출력
 */
	
	int [] arr = new int[5];
	
	for(int i=1; i<arr.length; i++) {
		arr[i] = (int)(Math.random()*5)+1;
		System.out.println("arr["+i+"] : " + arr[i]);
			
		}
	}
	

	}
}
