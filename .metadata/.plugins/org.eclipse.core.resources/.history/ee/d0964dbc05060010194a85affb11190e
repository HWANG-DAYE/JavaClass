package project0321;

public class RandomEx02 {
	public static void main(String[] args) {
		int number = (int)(Math.random()*3)+1;
//		System.out.println(number);
		String num = "";
		if(number==1) {
			num="가위";
		} else if(number==2) {
			num="바위";
		} else {
			num="보";
		}
		System.out.println(num);
		
		num = number==1? "가위" : number==2? "바위" : "보";
		System.out.println(num);
		
	}
}
